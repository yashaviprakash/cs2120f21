digraph G {
    n0 [label="0"]
    n1 [label="1"]
    n2 [label="2"]
    n3 [label="3"]
    n4 [label="4"]


    n0 -> n1
    n1 -> n2
    n2 -> n3
    n3 -> n4


    # reflexive
